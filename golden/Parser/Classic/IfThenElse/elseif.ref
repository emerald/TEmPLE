IfThenElse ((EVar "x",
             [Invoke (ProcInvoc (EVar "foo", "x", []))]),
            [(EVar "y",
              [Invoke (ProcInvoc (EVar "bar", "y", []))]),
             (EVar "x",
              [Invoke (ProcInvoc (EVar "baz", "z", []))])],
            Just [Invoke (ProcInvoc (EVar "z", "x", []))])
